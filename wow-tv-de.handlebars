            var paymentProviderCookie = cookies.filter(function (item) {
                return item.indexOf('paymentProvider=') >= 0
            });
            if (paymentProviderCookie.length) {
                hasBTPaymentProvider = paymentProviderCookie[0].split('=')[1].split(',').filter(function (cookie) { return cookie == 'BT' }).length
            }
        }
        var isElligibleForBoostPromotion = 0;
        if (false) {
            var userEntitlementCookie = cookies.filter(function (item) {
                return item.indexOf('userEntitlements=') >= 0
            })
            if (userEntitlementCookie.length) {
                isElligibleForBoostPromotion = userEntitlementCookie[0].indexOf('ENTERTAINMENT%2CACTIVATED') >= 0 && userEntitlementCookie[0].indexOf('HD%2CACTIVATED') < 0
            }
        }
        var hasKnownUserCookie = 0;
        if (true) {
            hasKnownUserCookie = cookies.filter(function (item) {
                return item.indexOf('KNOWN_USER=') >= 0
            }).length > 0
        }

        if (hasBTPaymentProvider) {
            null
        }
        else if (isElligibleForBoostPromotion) {
            null
        }
        else if (hasKnownUserCookie) {
            window.location.replace('https://www.wowtv.de/home/kunde' + window.location.search)
        }</script>
