    <style>
        html {
            background: #20313b;
        }

        body {
            font-family: 'Roboto', sans-serif;
            color: #d2d2d6;
        }

        html,
        body,
        .viewport {
            width: 100%;
            height: 100%;
            margin: 0;
        }

        #app {
            display: flex;
            justify-content: center;
            align-items: center;
            flex-direction: column;
        }

        #BrandLogo {
            height: 200px;
            width: 400px;
            background-image: url('/static/images/dev_logo.png');
            background-repeat: no-repeat;
            background-size: contain;
            background-position: center;
        }

        .content {
            flex-direction: column;
            flex-wrap: wrap;
            justify-content: center;
            align-items: center;
            font-weight: lighter;
        }

        p {
            text-align: center;
        }

        .section {
            margin-top: 30px;
            display: flex;
            flex-direction: column;
            flex-wrap: wrap;
            justify-content: center;
            align-items: center;
            font-weight: lighter;
        }

        .section h1,
        h2,
        h3,
        h4 {
            margin: 5px;
            text-align: center;
        }

        /*Pointlessly Pretty Button */

        a {
            text-decoration: none;
        }

        .btn {
            text-transform: uppercase;
            position: relative;
            font-weight: bold;
            display: block;
            margin: 30px auto;
            padding: 0;

            overflow: hidden;

            border-width: 0;
            outline: none;
            border-radius: 2px;
            box-shadow: 0 1px 4px rgba(0, 0, 0, .6);

            background-color: #e22866;
            color: #ecf0f1;

            transition: background-color .3s;

        }

        .btn:hover,
        .btn:focus {
            background-color: transparent;
            border: 2px #e22866 solid;
            cursor: pointer;
        }

        .btn>* {
            position: relative;
        }

        .btn span {
            display: block;
            padding: 12px 24px;
        }

        .btn:before {
            content: "";

            position: absolute;
            top: 50%;
            left: 50%;

            display: block;
            width: 0;
            padding-top: 0;

            border-radius: 100%;

            background-color: transparent;

            -webkit-transform: translate(-50%, -50%);
            -moz-transform: translate(-50%, -50%);
            -ms-transform: translate(-50%, -50%);
            -o-transform: translate(-50%, -50%);
            transform: translate(-50%, -50%);
        }

        .btn:active:before {
            width: 120%;
            padding-top: 120%;

            transition: width .2s ease-out, padding-top .2s ease-out;
        }

        @import url("https://fonts.googleapis.com/css2?family=Open+Sans&display=swap") font;
        @import url("https://use.fontawesome.com/releases/v5.7.1/css/all.css") fontawsome;

        .content {
            width: 100%;
            background-size: cover;
            display: flex;
            flex-wrap: nowrap;
            overflow: auto;
        }

        #BrandLogo {
            background-color: white;
            background-image: url("https://www.pocruises.com//content/dam/po/pno_logo.svg");
            background-repeat: no-repeat;
            background-size: 211px 77px;
            height: 134px;
            width: 100%;
            background-position: 22%;
            border-bottom: 2px solid #988642;
        }

        .section h1 {
            padding-top: 10px;
        }


        #app {
            display: flex;
        }

        html {
            background: #324672 url(data:image/svg+xml;base64,) repeat;
            background-size: 60px;
        }

        .section {
            background-color: white;
            width: 50%;
            margin-top: 0px;
            color: rgb(0, 0, 0);
            font-family: sans-serif;
            font-weight: 100;
            line-height: 19.2px
        }

        .section-hero {
            background-color: black;
            margin-top: 0px;
            color: rgb(0, 0, 0);
            font-family: sans-serif;
            font-weight: 100;
            line-height: 19.2px
        }

        .section img {
            width: -moz-available;
        }

        .btn {
            background: #b10f26 !important;
            letter-spacing: .05rem;
            color: #fff;
            text-transform: uppercase;
            cursor: pointer;
            outline: none;
            border: none;
            border-radius: 15px;
            display: block;
            margin: 0 auto;
            margin-bottom: 20px;
        }

        .btn-top-bar {
            background: #b10f26 !important;
            letter-spacing: .05rem;
            color: #fff;
            text-transform: uppercase;
            cursor: pointer;
            outline: none;
            border: none;
            border-radius: 15px;
            display: block;
            margin-right: 400px;
            margin-top: 70px
        }

        .btn:hover,
        .btn:focus {
            background-color: #808080;
            border: 0px #ed8b00 solid;
            border-color: transparent transparent #ed8b00;
            cursor: pointer;
        }

        .btn span::after,
        .cta span::before {
            position: absolute;
            top: -9px;
            border-width: 0 9px 14px;
        }

        .btn span::after {
            left: -15px;
        }
